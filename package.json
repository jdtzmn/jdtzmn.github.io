{
  "name": "jdtzmn.github.io",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "start": "nuxt start",
    "test": "concurrently --kill-others-on-fail \"npm:test:unit\" \"npm:test:e2e\"",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test dev 3000 test:cypress",
    "test:watch": "jest --watch",
    "test:cypress": "cypress run",
    "cypress": "cypress open",
    "dev": "nuxt",
    "debug": "node --nolazy --inspect ./node_modules/.bin/nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --fix .",
    "stylelint": "stylelint **/*.{scss,vue}",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add",
      "jest --findRelatedTests"
    ],
    "*.scss": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "nuxt-edge": "^2.0.0-25568071.0dff1b8",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@types/node": "^10.9.0",
    "@vue/test-utils": "^1.0.0-beta.21",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.0",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-typescript": "^1.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.0.3",
    "start-server-and-test": "^1.7.0",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0",
    "vue-jest": "^2.6.0"
  }
}
